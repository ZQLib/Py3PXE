default menu.c32
prompt 0
timeout 100
ONTIMEOUT TinyCore

MENU TITLE PyPXE

LABEL TinyCore
    MENU LABEL TinyCore
    KERNEL memdisk
    APPEND iso initrd=Core.iso

LABEL AgreeOS
    MENU LABEL AgreeOS
    KERNEL memdisk
    APPEND iso initrd=mini.iso

LABEL local
        MENU LABEL Boot local hard drive
        LOCALBOOT 0
