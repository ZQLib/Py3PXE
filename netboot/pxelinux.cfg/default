default menu.c32
prompt 0
timeout 100
ONTIMEOUT AgreeOS

MENU TITLE AgreeOS PXE

LABEL AgreeOS
    MENU LABEL Install AgreeOS From Network
    KERNEL kernel
    APPEND initrd=initrd.img quiet root=/dev/ram0 androidboot.selinux=permissive vmalloc=192M buildvariant=userdebug INSTALL=1 BYNET=1 DEBUG=

LABEL TinyCore
    MENU LABEL TinyCore
    KERNEL memdisk
    APPEND iso initrd=Core.iso

LABEL local
        MENU LABEL Boot local hard drive
        LOCALBOOT 0
