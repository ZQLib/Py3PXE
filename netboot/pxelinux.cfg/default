default menu.c32
prompt 0
timeout 50
ONTIMEOUT AosTest

MENU TITLE PyPXE

LABEL AosTest
    MENU LABEL AOS Test
    KERNEL kernel
    APPEND initrd=initrd.img root=/dev/ram0 androidboot.selinux=permissive vmalloc=192M buildvariant=userdebug SRC=/ZTS

LABEL AgreeOS
    MENU LABEL AgreeOS
    KERNEL memdisk
    APPEND iso initrd=mini.iso

LABEL TinyCore
    MENU LABEL TinyCore
    KERNEL memdisk
    APPEND iso initrd=Core.iso

LABEL local
        MENU LABEL Boot local hard drive
        LOCALBOOT 0
